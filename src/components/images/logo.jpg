ce-pre-wrap">.card-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
  gap: 20px;
  padding: 30px;
}

.card-item {
  opacity: 0;
  transform: translateY(40px) scale(0.95);
  transition: opacity 0.6s ease, transform 0.6s ease;
}

.card-item.show {
  opacity: 1;
  transform: translateY(0) scale(1);
}

.card-item:hover {
  transform: translateY(-10px);
  box-shadow: 0 12px 30px rgba(0, 0, 0, 0.25);
}

.image-card {
  border: none;
  overflow: hidden;
  border-radius: 15px;
  position: relative;
  height: 100%;
}

.card-img {
  height: 100%;
  width: 100%;
  object-fit: cover;
  filter: brightness(65%);
  transform: scale(1.1);
  transition: transform 1.2s ease-in-out;
}

.card-item:hover .card-img {
  transform: scale(1);
}

.card-overlay {
  color: white;
  text-shadow: 1px 1px 4px rgba(0, 0, 0, 0.7);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.35);
  transition: background-color 0.3s ease;
}

.card-item:hover .card-overlay {
  background-color: rgba(0, 0, 0, 0.45);
}

.card-overlay * {
  color: white !important;
}

.card-title {
  font-size: 1.2rem;
  font-weight: bold;
  margin-bottom: 10px;
  margin-top: 3px;
}

.card-overlay .btn-light {
  background-color: transparent !important;
  border: 1px solid white !important;
  color: white !important;
  font-weight: 500;
  font-size: 0.9rem;
  padding: 6px 16px;
  border-radius: 10px;
  transition: all 0.3s ease;
}

.card-overlay .btn-light:hover {
  background-color: #ff5722 !important;
  color: white !important;
  transform: scale(1.05);
}

.tall-card {
  grid-row: span 2;
}

@media (max-width: 768px) {
  .card-grid {
    grid-template-columns: 1fr;
    padding: 20px;
  }
} .animated-background {
  animation: bgZoom 40s ease-in-out infinite;
}

@keyframes bgZoom {
  0% {
    transform: scale(1) translate(0, 0);
  }
  50% {
    transform: scale(1.05) translate(-10px, -10px);
  }
  100% {
    transform: scale(1) translate(0, 0);
  }
}

.heading {
  color: white;
  display: flex;
  align-items: center;
  gap: 10px;
  font-weight: bold;
}

.card-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 25px;
  margin-top: 30px;
  position: relative;
}

.card-hover {
  position: relative;
  overflow: hidden;
  padding: 30px;
  border-radius: 20px;
  color: white;
  box-shadow: 0 6px 25px rgba(0, 0, 0, 0.3);
  backdrop-filter: blur(4px);
  cursor: default;
  opacity: 0;
  transition: transform 0.6s ease, opacity 0.6s ease;
}

/* Water shimmer ripple effect */
.card-hover::before {
  content: "";
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
  background: radial-gradient(circle, rgba(255, 255, 255, 0.05) 20%, transparent 70%);
  animation: rippleMove 12s linear infinite;
  pointer-events: none;
  z-index: 0;
}

@keyframes rippleMove {
  0% {
    transform: translate(-50%, -50%) scale(1);
  }
  100% {
    transform: translate(50%, 50%) scale(1.5);
  }
}

/* Direction-based entry */
.direction-0 {
  transform: translateY(100px); /* bottom */
}
.direction-1 {
  transform: translateX(-100px); /* left */
}
.direction-2 {
  transform: translateY(-100px); /* top */
}
.direction-3 {
  transform: translateX(100px); /* right */
}

.show {
  opacity: 1;
  transform: translate(0, 0);
}

.card-caption {
  position: relative;
  z-index: 1;
  opacity: 1;
  transform: translateY(0);
  transition: none;
}

.card-title {
  display: flex;
  align-items: center;
  gap: 10px;
  margin-bottom: 12px;
  font-size: 1.3rem;
}

.card-text {
  font-size: 16px;
  margin: 0;
