ta-multiline=""><div class="whitespace-pre-wrap">import React, { useEffect, useRef, useState } from "react";
import "./Tex.css";
import sasi from "./components/images/mounten.webp"

const cards = [
  { title: "Small Group Departures", subtitle: "Explore with like-minded travelers." },
  { title: "Affordable Dreams", subtitle: "Great value journeys await." },
  { title: "Undiscovered Tours", subtitle: "Hidden gems revealed." },
  { title: "Religious Tours", subtitle: "Spiritual paths across India." },
  { title: "Solo Travel", subtitle: "Freedom to roam solo." },
  { title: "Private Touring", subtitle: "Personalized luxury journeys." },
  { title: "Let India Unfold With Us", subtitle: "Not sure where to begin?" },
  { title: "Women Tours", subtitle: "Empowering travel experiences." },
];

const Cart = () =&gt; {
  const cardRefs = useRef([]);
  const [visibleCards, setVisibleCards] = useState(Array(cards.length).fill(false));

  useEffect(() =&gt; {
    const observer = new IntersectionObserver(
      (entries) =&gt; {
        const updated = [...visibleCards];
        entries.forEach((entry) =&gt; {
          const index = Number(entry.target.getAttribute("data-index"));
          if (entry.isIntersecting &amp;&amp; !updated[index]) {
            updated[index] = true;
          }
        });
        setVisibleCards(updated);
      },
      { threshold: 0.3 }
    );

    cardRefs.current.forEach((ref) =&gt; ref &amp;&amp; observer.observe(ref));
    return () =&gt; {
      cardRefs.current.forEach((ref) =&gt; ref &amp;&amp; observer.unobserve(ref));
    };
  }, [visibleCards]);

  return (
    &lt;div className="card-grid"&gt;
      {cards.map((card, index) =&gt; (
        &lt;div
          key={index}
          className={<code>card-hover direction-${index % 4} ${
</code>            visibleCards[index] ? "show" : ""
          }<code>}
</code>          ref={(el) =&gt; (cardRefs.current[index] = el)}
          data-index={index}
        &gt;
          &lt;div className="card-caption"&gt;
            &lt;h3 className="card-title"&gt;{card.title}&lt;/h3&gt;
            &lt;p className="card-text"&gt;{card.subtitle}&lt;/p&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      ))}
    &lt;/div&gt;
  );
};

export default Cart;.card-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 25px;
  margin-top: 30px;
  position: relative;
}

.card-hover {
  position: relative;
  overflow: hidden;
  padding: 30px;
  border-radius: 20px;
  color: white;
  background: linear-gradient(145deg, #333, #222);
  box-shadow: 0 6px 25px rgba(0, 0, 0, 0.3);
  backdrop-filter: blur(4px);
  cursor: default;
  opacity: 0;
  transition: transform 0.6s ease, opacity 0.6s ease;
}

/* Animated ripple background */
.card-hover::before {
  content: "";
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
  background: radial-gradient(circle, rgba(255, 255, 255, 0.05) 20%, transparent 70%);
  animation: rippleMove 12s linear infinite;
  pointer-events: none;
  z-index: 0;
}

@keyframes rippleMove {
  0% { transform: translate(-50%, -50%) scale(1); }
  100% { transform: translate(50%, 50%) scale(1.5); }
}

.card-caption {
  position: relative;
  z-index: 1;
  opacity: 1;
  transform: translateY(0);
}

.card-title {
  color: white;
  font-size: 1.3rem;
  font-weight: bold;
  margin-bottom: 12px;
}

.card-text {
  font-size: 16px;
  margin: 0;
  color: white;
}

/* Entry animation directions */
.direction-0 { transform: translateY(100px); }
.direction-1 { transform: translateX(-100px); }
.direction-2 { transform: translateY(-100px); }
.direction-3 { transform: translateX(100px); }

.show {
  opacity: 1;
  transform: translate(0, 0);
