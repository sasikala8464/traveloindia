      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">h5</span></span></span><span> </span><span><span class="hljs-attr">class</span></span><span>=</span><span><span class="hljs-string">"card-title"</span></span><span>&gt;Card Title</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">h5</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">p</span></span></span><span>&gt;Some card text here</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">p</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">button</span></span></span><span> </span><span><span class="hljs-attr">class</span></span><span>=</span><span><span class="hljs-string">"btn"</span></span><span>&gt;View</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">button</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
